<?xml version="1.0" ?>
<odoo noupdate="1">
    <record id="ir_cron_cloud_phone_day_scheduler" model="ir.cron">
      <field name="interval_type">days</field>
      <field name="name">Cloud phone connectors scheduler 60 minutes</field>
      <field name="numbercall">-1</field>
      <field name="interval_number">1</field>
      <field name="doall" eval="False"/>
      <field name="active" eval="False"/>
      <field name="model_id" ref="model_cloud_phone_connector"/>
      <field name="state">code</field>
      <field name="code"># days=None, minutes=60, wait_minutes=60, recreate=False
# days period. now-days
# minutes period. now-minutes
# wait_minutes. Request only ended calls
# recreacte. If has call with rec_duration>00:00:00 and attachment-False
# try request attachment(recording)
model.schedule_connector(days=None, minutes=60, wait_minutes=60)</field>
    </record>

